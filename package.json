{
  "name": "film-metadata-app",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "description": "Film Metadata App",
  "author": "Hyunsoo Roh",
  "license": "MIT",
  "homepage": "https://film-metadata.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/toosign00/film-metadata-app"
  },
  "keywords": [
    "film",
    "metadata",
    "app",
    "film-metadata",
    "film-metadata-app",
    "tailwindcss",
    "react",
    "typescript"
  ],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "compile": "pnpm clean && pnpm build && pnpm preview",
    "check": "concurrently \"pnpm:type-check\" \"pnpm:lint\" \"pnpm:format\"",
    "fix": "pnpm lint:fix && pnpm format:fix",
    "format": "prettier --check . --cache",
    "format:fix": "prettier --write . --cache",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --cache",
    "lint:fix": "eslint . --ext ts,tsx --fix --cache",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "date-fns": "^4.1.0",
    "fflate": "^0.8.2",
    "file-saver": "^2.0.5",
    "piexifjs": "^1.0.6",
    "react": "^19.1.0",
    "react-datepicker": "^8.4.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@tailwindcss/vite": "^4.1.8",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.5.1",
    "concurrently": "^9.2.0",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "npm-check-updates": "^18.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^6.3.5"
  }
}
