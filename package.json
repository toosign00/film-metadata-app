{
  "name": "film-metadata-app",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "module": "ES2022",
  "description": "Film Metadata App",
  "author": "Hyunsoo Roh",
  "license": "MIT",
  "homepage": "https://film-metadata.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/toosign00/film-metadata-app"
  },
  "keywords": [
    "film",
    "metadata",
    "app",
    "film-metadata",
    "film-metadata-app",
    "tailwindcss",
    "react",
    "typescript"
  ],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "compile": "pnpm clean && pnpm build && pnpm preview",
    "lint": "biome lint --write",
    "format": "biome format --write",
    "check": "pnpm type-check && biome check --write",
    "type-check": "tsc --noEmit --project tsconfig.app.json",
    "reporter": "biome check --reporter=summary"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@vercel/analytics": "^1.5.0",
    "date-fns": "^4.1.0",
    "fflate": "^0.8.2",
    "file-saver": "^2.0.5",
    "lucide": "^0.536.0",
    "lucide-react": "^0.536.0",
    "piexifjs": "^1.0.6",
    "react": "^19.1.1",
    "react-datepicker": "^8.4.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.1.1",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "npm-check-updates": "^18.0.2",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5.9.2",
    "vite": "^7.0.6"
  }
}
